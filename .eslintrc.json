{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "import/no-extraneous-dependencies": "off",
        "indent": "off",
        "eol-last": "off",
        "no-trailing-spaces": "off",
        "import/newline-after-import": "off"
        // "padding-line-between-statements": [
        //     "error",
        //     {
        //       "blankLine": "never",
        //       "prev": "import",
        //       "next": "import"
        //     }
        //   ]
    }
}